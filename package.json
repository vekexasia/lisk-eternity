    "gulp-util": "^3.0.8",
    "happypack": "^5.0.0-beta.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "json-loader": "^0.5.7",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
