{
  "name": "lisketernity",
  "version": "0.0.1",
  "description": "Lisk Eternity Wall",
  "author": "Andrea <vekexasia@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepush": "npm run ilint -q",
    "dll": "npx rimraf dist && npx webpack --config webpack.dll.config.js",
    "postinstall": "npm -s run dll",
    "deploy": "npm run build; (date +'%Y-%m-%d %H:%M:%S' > dist/build.txt); gh-pages -d dist",
    "dev": "npx cross-env NODE_ENV=development node ./build/dev-server.js",
    "build": "npx rimraf dist && npx cross-env NODE_ENV=production npx webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "ilint": "npx tslint -p ./tsconfig.json --type-check src/**/*.ts && npx eslint src/**/*.vue",
    "fix": "npx tslint -p ./tsconfig.json --type-check --fix src/**/*.ts && npx eslint --fix src/**/*.vue"
  },
  "dependencies": {
    "@ledgerhq/hw-transport-u2f": "^4.21.0",
    "async-await-error-handling": "^0.5.2",
    "axios": "^0.18.0",
    "dpos-ledger-api": "^1.1.0",
    "dpos-offline": "^2.0.0",
    "lisk-elements": "^1.0.0",
    "mobx": "^4.0.1",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "socket.io-client": "^2.1.1",
    "sprintf": "^0.1.5",
    "vue": "^2.5.16",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-mobx": "^2.0.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@types/node": "^9.4.7",
    "@types/sprintf": "^0.1.0",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
